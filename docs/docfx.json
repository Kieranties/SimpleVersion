{
  "metadata": [
    {
      "src": [
        {
          "files": "**/bin/**/Simpleversion*dll",
          "src" : "../src"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.{yml,md}",
          "articles/**.{yml,md}",
          "*.{yml,md}"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "theme/**"
        ]
      }
    ],
    "overwrite":  "apidoc/**.md",
    "dest": "obj/site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "./theme"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appFaviconPath": "images/log.png",
      "_appLogoPath": "images/logo.png",
      "_appName": "SimpleVersion"
    }
  }
}
