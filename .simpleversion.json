{
  "version": "0.1.0",
  "metadata": ["*"],
  "branches": {
    "release": [
      "^refs/heads/master$",
      "^refs/heads/preview/.+$",
      "^refs/heads/release/.+$"
    ],
    "overrides": [
      {
        "match": "^refs/heads/feature/.+$",
        "metadata": [ "{shortbranchname}" ]
      },
      {
        "match": "^refs/heads/release/.+$",
        "label": [],
        "metadata": [ "*" ]
      }
    ]
  }
}