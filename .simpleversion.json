{
  "version": "0.4.0",
  "branches": {
    "release": ["^refs/heads/(master$|release/.+$)"],
    "overrides": [
      {
        "match": "^refs/heads/master$",
        "label": ["beta", "1"]
      },
      {
        "match": "^refs/heads/(feature|bug)/.+$",
        "label": ["{shortbranchname}"]
      },
      {
        "match": "^refs/heads/release/.+$"
      }
    ]
  }
}
